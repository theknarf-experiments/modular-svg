{
	"compilerOptions": {
		"incremental": true,
		"target": "es2022",
		"lib": ["es2022"],
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"types": ["node"],
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"skipLibCheck": true
	}
}
